{
  "name": "balance-software",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "turbo:dev": "next dev --turbo",
    "build": "next build",
    "turbo:build": "turbo build",
    "start": "next start",
    "turbo:start": "turbo start",
    "lint": "next lint --fix",
    "turbo:lint": "turbo lint:fix",
    "prettier": "prettier --write \"**/*.{ts,tsx,json}\" --ignore-unknown",
    "typecheck": "tsc --noEmit",
    "turbo:typecheck": "turbo typecheck",
    "format": "npm run prettier && npm run lint && npm run typecheck",
    "turbo:format": "npm run prettier && npm run turbo:lint && npm run turbo:typecheck",
    "biome:check": "biome check --apply ./src",
    "biome:format": "biome format --write ./src",
    "biome:lint": "biome lint ./src"
  },
  "dependencies": {

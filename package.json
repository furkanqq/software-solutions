    "axios": "^1.6.8",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.10",
    "fs": "^0.0.1-security",
    "gsap": "^3.12.5",
    "handlebars": "^4.7.8",
    "html-react-parser": "^5.1.10",
    "jotai": "^2.7.1",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "next": "^14.1.3",
    "next-seo": "^6.5.0",
    "nodemailer": "^6.9.14",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",

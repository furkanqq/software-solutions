    "eslint-plugin-react": "^7.34.1",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "turbo": "^1.12.5",
    "typescript": "^5.4.2"
  },
  "browserslist": {
    "production": [
      "last 15 version",
      "> 1%",
      "IE 10"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
